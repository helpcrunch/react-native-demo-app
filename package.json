    "update-pods": "cd ios && pod install --repo-update && cd .. "
  },
  "dependencies": {
    "@klarna/react-native-vector-drawable": "^0.5.1",
    "@preact/signals-react": "^3.0.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.6.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "6.9.17",
    "@react-navigation/stack": "6.3.20",
    "react": "19.0.0",
    "react-native": "0.78.0",
    "react-native-drawer-layout": "^4.1.6",
    "react-native-gesture-handler": "^2.24.0",
    "react-native-reanimated": "^3.17.1",
    "react-native-safe-area-context": "^5.3.0",
    "react-native-screens": "^4.9.2",
    "react-native-swipe-gestures": "^1.0.5",
    "react-native-vector-image": "^0.4.5",
    "react-native-webview": "^13.13.4",
    "rn-helpcrunch": "^0.1.20"
