{
  "name": "testhelpcrunch",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android -- --enable-native-access=ALL-UNNAMED",
    "ios": "react-native run-ios --simulator=\"iPhone 16\" --mode Debug",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "generate-images": "react-native-vector-image generate",
    "install-pods": "cd ios && pod install && cd .. ",
